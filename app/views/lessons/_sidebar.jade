//=require bootstrap-tabs bootstrap-scrollspy
.row-fluid
		.sidebar.well
			ul.nav.nav-list( data-spy="scroll" )
				li.nav-header 
					i.icon-book
					| Course Content
				- var chapters = course.chapters || []
				each chapter in chapters
					li
						li.nav-header 
							a(href='/chapter/#{chapter.id}/') #{chapter.title}
				
				li.divider
				li.nav-header 
						i.icon-pencil
						| Tool
					li
						a(href='/program/#{course.id}/1/1') 
							| Example IDE
					li
						a(href='/quiz/#{course.id}/1/2') 
							| Example QUIZ

